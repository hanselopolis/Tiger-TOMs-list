<h1>Search results template</h1>
<table class="table table-hover">
  <thead class="thead-dark">
    <tr>
      <th>Title</th>
      <th>Post</th>
    </tr>
  </thead>
  <tbody>
    <% @ads.each do |ad| %>
      <tr>
        <td><%= ad.title %></td>
        <td><%= ad.created_at.strftime("%m/%d/%Y") %></td>
      </tr>
    <% end %>
  </tbody>
</table>